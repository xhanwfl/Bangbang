<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <corners
                android:radius = "12dp" />
            <solid
                android:color = "#FFFFFF"
                />
            <stroke
                android:width="1dp"
                android:color="#DFDFDF"/>
        </shape>

    </item>
    <item android:state_checked="false">
        <shape>
            <corners
                android:radius = "12dp" />
            <solid
                android:color = "#DFDFDF"
                />
        </shape>
    </item>
</selector>

